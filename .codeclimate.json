{
  "version": 2,
  "exclude_patterns": [
    "data",
    "migrations",
    "node_modules",
    "scripts",
    "temp",
    "test",
    "integration-tests",
    "src/modules/acceptance-tests",
    "src/modules/**/routes.js",
    "src/lib/connectors/bookshelf",
    "src/modules/licences/routes"
  ],
  "checks": {
    "method-count": {
      "config": {
        "threshold": 30
      }
    }
  }
}
